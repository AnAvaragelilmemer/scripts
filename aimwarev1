if __Tors.Enabled then
for i,v in pairs(game:service'Players':GetPlayers()) do
if v.TeamColor ~= game:service'Players'.LocalPlayer.TeamColor then
--if v.Name ~= game:service'Players'.LocalPlayer.Name then
pcall(function()
